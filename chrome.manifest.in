overlay chrome://browser/content/browser.xul chrome://weave/content/sync.xul
overlay chrome://browser/content/preferences/preferences.xul chrome://weave/content/preferences.xul

@unpacked@content weave chrome/content/
@unpacked@skin weave class/1.0 chrome/skin/
@unpacked@locale playground en-US chrome/locale/en-US/

@jar@content weave jar:chrome/sync.jar!/content/
